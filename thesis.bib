	Eprint = {http://biorxiv.org/content/early/2017/04/15/127720.full.pdf},
	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Labs Journals},
	Title = {StateHub-StatePaintR: rules-based chromatin state annotations.},
